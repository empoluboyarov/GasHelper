<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".hardcounts.CalcProduvkaActivity">

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:text="Абсолютная плотность газа"
        android:textSize="17dp" />

    <EditText
        android:id="@+id/etRoProd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView12"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/textView12"
        android:ems="10"
        android:textSize="17dp"
        android:inputType="numberDecimal" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView12"
        android:layout_marginTop="10dp"
        android:text="Молярная сост азота"
        android:textSize="17dp" />

    <EditText
        android:id="@+id/etAzotProd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView15"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/textView12"
        android:ems="10"
        android:inputType="numberDecimal" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView15"
        android:layout_marginTop="10dp"
        android:text="Избыточное давление газа"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView16"
        android:layout_marginTop="10dp"
        android:text="Атмосферное давление"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView17"
        android:layout_marginTop="10dp"
        android:text="Температура газа"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView18"
        android:layout_marginTop="10dp"
        android:text="Температура грунта"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView19"
        android:layout_marginTop="10dp"
        android:text="Длина свечной линии"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView20"
        android:layout_marginTop="10dp"
        android:text="Внут диаметр свечной линии"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView21"
        android:layout_marginTop="10dp"
        android:text="Внут диаметр свечного крана"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView22"
        android:layout_marginTop="10dp"
        android:text="Время истечения газа"
        android:textSize="17dp" />

    <EditText
        android:id="@+id/etPnProd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView17"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etAzotProd"
        android:layout_toEndOf="@+id/textView16"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etPrtProd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView18"
        android:layout_alignEnd="@+id/etAzotProd"
        android:layout_alignStart="@+id/etPnProd"
        android:layout_toEndOf="@+id/textView16"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etTnProd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView19"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etPrtProd"
        android:layout_toEndOf="@+id/textView17"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etTgrProd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView20"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etTnProd"
        android:layout_toEndOf="@+id/textView18"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etLProd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView20"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etTgrProd"
        android:layout_toEndOf="@+id/textView19"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etDProd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView22"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etLProd"
        android:layout_toEndOf="@+id/textView20"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etDkProd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView23"
        android:layout_alignEnd="@+id/etAzotProd"
        android:layout_alignStart="@+id/etDProd"
        android:layout_toEndOf="@+id/textView21"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etTimProd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView23"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etDkProd"
        android:layout_toEndOf="@+id/textView22"
        android:ems="10"
        android:inputType="numberDecimal" />


    <Button
        android:id="@+id/btCalcAv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:onClick="calcProduvka"
        android:text="Расчет"
        android:layout_below="@+id/textView23" />

    <TextView
        android:id="@+id/tvResultProd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btCalcAv"
        android:layout_centerHorizontal="true"
        android:text="Приведение к 20 град и 760 ммртст"
        android:textAppearance="?android:attr/textAppearanceMedium" />

</RelativeLayout>
