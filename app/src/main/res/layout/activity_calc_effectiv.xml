<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="empoluboyarov.com.gashelper.CalcEffectivActivity">


    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:text="Абсолютная плотность газа"
        android:textSize="17dp" />

    <EditText
        android:id="@+id/etRoEf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView12"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/textView12"
        android:ems="10"
        android:textSize="17dp"
        android:inputType="numberDecimal" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView12"
        android:layout_marginTop="10dp"
        android:text="Давление в начале"
        android:textSize="17dp" />

    <EditText
        android:id="@+id/etPnEf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView15"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/textView12"
        android:ems="10"
        android:inputType="numberDecimal" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView15"
        android:layout_marginTop="10dp"
        android:text="Давление в конце"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView16"
        android:layout_marginTop="10dp"
        android:text="Атмосферное давление"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView17"
        android:layout_marginTop="10dp"
        android:text="Температура в начале"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView18"
        android:layout_marginTop="10dp"
        android:text="Температура в конце"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView19"
        android:layout_marginTop="10dp"
        android:text="Температура грунта"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView20"
        android:layout_marginTop="10dp"
        android:text="Транспорт газа"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView21"
        android:layout_marginTop="10dp"
        android:text="Длина газопровода"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView22"
        android:layout_marginTop="10dp"
        android:text="Внутренний диаметр"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView23"
        android:layout_marginTop="10dp"
        android:text="Молярная сост азота"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView25"
        android:layout_marginTop="10dp"
        android:text="Высота - начало МГ"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView26"
        android:layout_marginTop="10dp"
        android:text="Высота  - конец МГ"
        android:textSize="17dp" />

    <EditText
        android:id="@+id/etPkEf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView17"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etPnEf"
        android:layout_toEndOf="@+id/textView16"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etPrtEf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView18"
        android:layout_alignEnd="@+id/etPnEf"
        android:layout_alignStart="@+id/etPkEf"
        android:layout_toEndOf="@+id/textView16"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etTnEf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView19"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etPrtEf"
        android:layout_toEndOf="@+id/textView17"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etTkEf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView20"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etTnEf"
        android:layout_toEndOf="@+id/textView18"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etTgrEf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView20"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etTkEf"
        android:layout_toEndOf="@+id/textView19"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etQfEf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView22"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etTgrEf"
        android:layout_toEndOf="@+id/textView20"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etLEf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView23"
        android:layout_alignEnd="@+id/etPnEf"
        android:layout_alignStart="@+id/etQfEf"
        android:layout_toEndOf="@+id/textView21"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etDEf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView23"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etLEf"
        android:layout_toEndOf="@+id/textView22"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etAzotEf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView25"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etLEf"
        android:layout_toEndOf="@+id/textView23"
        android:ems="10"
        android:inputType="numberDecimal" />

    <EditText
        android:id="@+id/etHnEf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView26"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etLEf"
        android:layout_toEndOf="@+id/textView25"
        android:ems="10"
        android:inputType="numberDecimal" />


    <EditText
        android:id="@+id/etHkEf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView27"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/etLEf"
        android:layout_toEndOf="@+id/textView26"
        android:ems="10"
        android:inputType="numberDecimal" />


    <Button
        android:id="@+id/btCalcAv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView27"
        android:layout_centerHorizontal="true"
        android:onClick="calcEffectiv"
        android:text="Расчет" />

    <TextView
        android:id="@+id/tvResultEf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btCalcAv"
        android:layout_centerHorizontal="true"
        android:text="Приведение к 20 град и 760 ммртст"
        android:textAppearance="?android:attr/textAppearanceMedium" />


</RelativeLayout>
