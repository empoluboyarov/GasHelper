<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="empoluboyarov.com.gashelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".simplecounts.CalcZActivity"
            android:label="Коэффициент сжимаемости "
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".simplecounts.CalcAdiabataActivity"
            android:label="Коэффициент адиабаты"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".simplecounts.CalcFactDensActivity"
            android:label="Фактическая плотность газа"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".simplecounts.CalcGasCountActivity"
            android:label="Запас газа в газопроводe"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".methodics.BaseMethodicsActivity"
            android:label="@string/title_activity_base_methodics"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".simplecounts.CalcSpeedSoundActivity"
            android:label="Cкорость звука в газе"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".simplecounts.CalcSpeedGasActivity"
            android:label="Cкорость истечения газа"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".hardcounts.DynamicCountActivity"
            android:label="Запас газа в действующем газопроводе"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".hardcounts.ConvertVolumeActivity"
            android:label="Пересчет объема газа для счетчика"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".hardcounts.CalcAverageActivity"
            android:label="Среднее давление и средняя температура"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".hardcounts.CalcEffectivActivity"
            android:label="Гидравлическая эффективность МГ"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".hardcounts.CalcPropuskActivity"
            android:label="Пропускная способность участка МГ"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".hardcounts.CalcProduvkaActivity"
            android:label="Продувка оборудования"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".DewPointActivity"
            android:label="Пересчет точки росы"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>